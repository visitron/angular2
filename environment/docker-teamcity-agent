FROM jetbrains/teamcity-agent:2018.1.1
RUN apt update
RUN apt install wget
RUN wget 'https://download.java.net/java/early_access/jdk11/28/GPL/openjdk-11+28_linux-x64_bin.tar.gz' -o /tmp/jdk-11.tar.gz
#RUN apt update && \
# apt install wget && \
# wget 'https://download.java.net/java/early_access/jdk11/28/GPL/openjdk-11+28_linux-x64_bin.tar.gz' -o /tmp/jdk-11.tar.gz && \
RUN tar -xvf /tmp/jdk-11.tar.gz -o /usr/lib/jdk-11
#CMD ls /tmp